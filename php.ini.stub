[PHP]
memory_limit=3G
post_max_size=3G
upload_max_filesize=3G
max_file_uploads=999
max_input_vars=99999

error_reporting=-1
display_errors=On
display_startup_errors=On
ignore_repeated_errors=On
log_errors=On
html_errors=Off
max_execution_time=360

[Date]
date.timezone="Asia/Jakarta"

[curl]
curl.cainfo="C:/waap/cacert.pem"

[openssl]
openssl.cafile="C:/waap/cacert.pem"
